//The following C++ program will output the 
//multiplication table as show below:

//1*1=1          2*1=2                   3*1=3         ……  9*1=1
//1+2=2         2*2=4                   3*2=6          ……  9*2=18
//…….           …….                   …….           ……  …….
//1*9=9         2*8=18                 3*9=27         ……  9*9=81


#include<cstdlib>
#include<iostream>
#include<cmath>


using namespace std;

void multable(){
	
	int i,j;
	
	for(i=1;i<10;i++){
	
	for(j=1;j<10;j++)

	cout<<j<<"*"<<i<<"="<<j*i<<"\t";
	cout<<"\n";}
  
   }

  int main(int argc, char*argv[])
   
  {
  
   multable();

   system("PAUSE");
return EXIT_SUCCESS;

}
